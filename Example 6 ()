#include <stdio.h>  // Include the standard input/output library

void main()  // Define the main function
{
    float a, b;  // Declare two floating-point variables to store the user input

    // Take input from the user
    printf("Enter the value of a:  ");  // Prompt the user to enter the first value
    scanf("%f", &a);  // Read the input and store it in variable 'a'

    printf("Enter the value of b:  ");  // Prompt the user to enter the second value
    scanf("%f", &b);  // Read the input and store it in variable 'b'

    // Check if the values are equal
    if (a == b)  // Use an if statement to check if 'a' is equal to 'b'
    {
        printf("The above values are Equal.\n");  // Print message if the values are equal
    }
    else  // If the values are not equal
    {
        printf("The above values are NOT Equal.\n");  // Print message if the values are not equal
    }
}
