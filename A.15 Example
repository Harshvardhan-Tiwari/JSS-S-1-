#include <stdio.h>

void main() {
    int n;  // Number of terms in the Fibonacci series
    int t1 , t2, nextTerm;  // Initialize the first two terms and a placeholder for the next term

t1=0;
t2=1;

  
    printf("Enter the number of terms: ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");

    for (int i = 1; i <= n; i = i + 1)
    
     { 
       printf("%d ", t1);  // Print the current term
        
        nextTerm = t1 + t2;  // Calculate the next term
        t1 = t2;  // Update t1 to the current termD
        t2 = nextTerm;  // Update t2 to the new term
    }
}


