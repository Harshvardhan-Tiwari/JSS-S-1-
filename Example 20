#include<stdio.h>
int main()
{

 int n,sum=0;
 
 printf("Enter the  number of elemets:");
 scanf("%d"&n);

 int arr[n];

 printf("Enter the elements of the array:\n ")

 for(int i=0 ; i<n ; i++)
    {
    print("Enter %d element:\n",i);  
    scanf("%d",&arr[i]);
    }

  for(int j=0 ; j<n ; j++) 
     {
     sum=sum+arr[j];
     } 

     printf("The sum of all elemets of the array is: %d",sum);
 
}
